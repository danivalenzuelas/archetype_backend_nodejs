{
  "name": "integraciones_backend_cl",
  "version": "1.0.0",
  "description": "API Restful de integraciones para Chile (NodeJS)",
  "keywords": [
    "API",
    "Restfull",
    "NodeJS",
    "MongoDB",
    "Express",
    "JWT",
    "SocketIO"
  ],
  "author": {
    "email": "daniel.valenzuela@nubox.com",
    "name": "Nubox SpA",
    "url": "https://www.nubox.com"
  },
  "contributors": [
    "Daniel Valenzuela <daniel.valenzuela@nubox.com>"
  ],
  "license": "Apache-2.0",
  "private": true,
  "homepage": "https://www.nubox.com",
  "main": "app.js",
  "repository": {
    "type": "GIT",
    "url": "https://github.com/nubox-spa/integraciones_backend_cl"
  },
  "bugs": {
    "email": "daniel.valenzuela@nubox.com",
    "url": "https://github.com/nubox-spa/integraciones_backend_cl/issues"
  },
  "engines": {
    "node": ">=10.1.0",
    "npm": ">=6.1.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "coveragePathIgnorePatterns": [
      "app.js",
      "api/schedule.js",
      "api/socket.io.js",
      "config/api.js",
      "config/credentials.js",
      "config/passport.js",
      "config/settings.js",
      "coverage",
      "node_modules",
      "public/index.routes.js",
      "task/pretest.js",
      "task/seed.js",
      "utils/google.js"
    ]
  },
  "scripts": {
    "lint": "eslint app.js api/*.js config/*.js public/*.js utils/*.js api/**/*.js",
    "lint:watch": "esw app.js api/*.js config/*.js public/*.js utils/*.js api/**/*.js -w",
    "message:commit": "echo 'Starting ... npm run lint & npm run test'",
    "message:seed": "echo 'Starting ... Populate seeds'",
    "postseed": "pkill -f node",
    "postseed:dev": "pkill -f node",
    "preprod": "npm run lint & npm run test",
    "prestart": "pkill -f node",
    "pretest": "cross-env NODE_ENV=pretest node task/pretest.js",
    "pretest:dev": "cross-env NODE_ENV=pretest MONGO_URI=mongodb://localhost/integraciones_backend_cl node task/pretest.js",
    "prod": "cross-env NODE_ENV=production node app.js",
    "seed": "cross-env NODE_ENV=seed node task/seed.js",
    "seed:dev": "cross-env NODE_ENV=seed MONGO_URI=mongodb://localhost/integraciones_backend_cl node task/seed.js",
    "start": "cross-env NODE_ENV=development MONGO_URI=mongodb://localhost/integraciones_backend_cl nodemon app.js & npm run lint:watch",
    "test": "cross-env NODE_ENV=testing jest --detectOpenHandles --verbose --forceExit",
    "test:dev": "cross-env NODE_ENV=testing MONGO_URI=mongodb://localhost/integraciones_backend_cl jest --detectOpenHandles --verbose --forceExit"
  },
  "pre-commit": [
    "message:commit",
    "lint"
  ],
  "dependencies": {
    "bluebird": "3.5.4",
    "body-parser": "1.19.0",
    "cross-env": "5.2.0",
    "csvjson": "5.1.0",
    "deep-extend": "0.6.0",
    "express": "4.17.0",
    "express-prettify": "0.1.1",
    "forever": "1.0.0",
    "method-override": "3.0.0",
    "moment": "2.24.0",
    "mongoose": "5.5.9",
    "node-schedule": "1.3.2",
    "raven": "2.6.4",
    "request": "2.88.0",
    "request-promise": "4.2.4",
    "socket.io": "2.2.0",
    "supertest": "4.0.2",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.13.0",
    "eslint-watch": "5.1.2",
    "jest": "24.8.0",
    "nodemon": "1.19.0",
    "parallelshell": "3.0.2",
    "pre-commit": "1.2.2"
  }
}
