{
  "env": {
    "jest": true
  },
  "extends": "airbnb",
  "rules": {
    "arrow-body-style": 0,
    "consistent-return": 0,
    "func-names": [ "error", "never" ],
    "global-require": 0,
    "import/no-dynamic-require": 0,
    "max-len": 0,
    "new-cap": 0,
    "no-irregular-whitespace": 0,
    "no-lonely-if": 0,
    "no-nested-ternary": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "operator-linebreak": 0
  }
}
